 -- Constant: color-formatter-xcms-cie-xyz
 -- Constant: color-formatter-xcms-cie-xyy
 -- Constant: color-formatter-xcms-cie-luv
 -- Constant: color-formatter-xcms-cie-lab
 -- Constant: color-formatter-xcms-cie-lch
 -- Constant: color-formatter-xcms-rgbi
 -- Constant: color-formatter-xcms-rgb
